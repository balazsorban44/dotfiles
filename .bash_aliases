#X1 YOGA 2nd GEN (OLED)
alias b="xrandr --output eDP-1 --brightness $1"

# OS stuff
alias update="sudo apt update && sudo apt upgrade"
alias install="sudo dpkg -i $1"
alias open="nautilus ."

# Supr lazy
alias c="clear"
alias s="yarn start"

# Move around
alias ..="cd .."
alias pr="cd;cd projects"
alias dl="cd;cd Downloads"

# Git related stuff
alias gs="git status"
alias add="git add $1"
alias gr="git reset"
alias pull="git pull"
alias push="git push"
alias gc="git commit -m $1"

# Text editor
alias code="code ."

